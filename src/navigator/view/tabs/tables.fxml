        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="443.0" prefWidth="712.0">
            <TableView fx:id="driverCarTableView" editable="true" prefHeight="410.0" prefWidth="712.0">
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                </columnResizePolicy>
            </TableView>
            <Button fx:id="addDriverCarButton" layoutX="14.0" layoutY="420.0" mnemonicParsing="false"
                    text="Добавить запись"/>
            <Button fx:id="deleteDriverCarButton" layoutX="169.0" layoutY="420.0" mnemonicParsing="false"
                    text="Удалить запись"/>
        </AnchorPane>
    </Tab>
</TabPane>
</AnchorPane>
